{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier/vue"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/attribute-hyphenation": ["always", {
      "ignore": [
        "~mainContent",
        "~drawerContent",
        "gesturesEnabled",
        "drawerLocation"
      ]
    }]
  }
}
